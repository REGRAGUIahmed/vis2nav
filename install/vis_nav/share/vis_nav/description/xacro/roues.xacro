<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

<xacro:property name="my_param" value="0.001" />
<!-- rear left -->

<joint name="rear_left_wheel_joint" type="continuous">
    <parent link="chassis2"/>
    <child link="rear_left"/>
    <origin xyz="0.0 0.065 -0.02" rpy="-${pi/2} 0 0"/>
    <axis xyz="0 0 1"/>
</joint>

<link name="rear_left">
    <visual>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry >
        <material name="black"/>
    </visual>
    <collision>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.044070654" length="0.025" radius="0.0335">
    <!-- <xacro:inertial_cylinder mass="0.08" length="0.025" radius="0.0335"> -->
        <origin xyz="0.0 0.065 -0.02" rpy="0.0 0.0 0.0"/>
    </xacro:inertial_cylinder>

</link>
<gazebo reference="rear_left">
    <material>Gazebo/Black</material>
        <!-- <mu1 value="100000.0"/>
        <mu2 value="100000.0"/> -->
</gazebo>

<!-- rear right-->

<joint name="rear_right_wheel_joint" type="continuous">
    <parent link="chassis2"/>
    <child link="rear_right"/>
    <origin xyz="0.0 -0.065 -0.02" rpy="${pi/2} 0 0"/>
    <axis xyz="0 0 -1"/>
</joint>

<link name="rear_right">
    <visual>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.044070654" length="0.025" radius="0.0335">
    <!-- <xacro:inertial_cylinder mass="0.08" length="0.025" radius="0.0335"> -->
        <origin xyz="0.0 -0.065 -0.02" rpy="0.0 0.0 0.0"/>
    </xacro:inertial_cylinder>
</link>

<gazebo reference="rear_right">
    <material>Gazebo/Black</material>
        <!-- <mu1 value="100000.0"/>
        <mu2 value="100000.0"/> -->
</gazebo>




