<!-- front left -->

<joint name="caster_wheel1_joint" type="fixed">
    <parent link="chassis4"/>
    <child link="caster_wheel1"/>
    <origin xyz="0.0 0.065 -0.02" rpy="-${pi/2} 0 0"/>
    <axis xyz="0 0 1"/> 
</joint>

<link name="caster_wheel1">
    <visual>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry>
        <material name="black"/>
    </visual>
    <collision>
        <geometry>
            <cylinder length="0.025" radius="0.0335" />
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.044070654" length="0.025" radius="0.0335">
        <origin xyz="0.0 0.065 -0.02" rpy="0.0 0.0 0.0"/>
    </xacro:inertial_cylinder>
</link>

<gazebo reference="caster_wheel1">
    <material>Gazebo/Black</material>
    <mu1 value="${my_param}"/>
    <mu2 value="${my_param}"/>
</gazebo>

<!-- front right -->

<joint name="caster_wheel2_joint" type="fixed">
