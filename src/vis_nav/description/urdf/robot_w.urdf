      <wheel_separation>0.165</wheel_separation>
      <wheel_diameter>0.067</wheel_diameter>
      <wheel_diameter>0.067</wheel_diameter>
      <updateRate>50.0</updateRate>
      <max_wheel_acceleration>0.3</max_wheel_acceleration>
      <robot_base_frame>base_link</robot_base_frame>
