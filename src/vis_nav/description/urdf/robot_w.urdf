        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
    <visual>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
            <material name="green"/>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
